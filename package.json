{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "build": "bun run build.ts",
    "test": "bun test",
    "test:unit": "bun test tests/unit.test.ts",
    "test:e2e": "bun test tests/phase1.test.ts",
    "test:watch": "bun test --watch",
    "ci": "bun install && bun run build && bun run test:unit",
    "ci:full": "bun install && bun run build && bun test",
    "typecheck": "bunx tsc --noEmit",
    "typecheck:strict": "bunx tsc --noEmit --strict"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@xyflow/react": "^12.9.3",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.545.0",
    "react": "^19",
    "react-dom": "^19",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@browserbasehq/stagehand": "^3.0.3",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0"
  }
}
