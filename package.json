{
  "name": "project-xy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "build": "bun run build.ts",
    "test": "bun test",
    "test:unit": "bun test tests/unit.test.ts tests/layout.test.ts tests/phase3.test.ts tests/schemas.test.ts",
    "test:e2e": "bun test tests/persistence.test.ts",
    "test:playwright": "bun test tests/phase1.test.ts",
    "test:ai": "bun test tests/integration.test.ts",
    "test:watch": "bun test --watch",
    "ci": "bun install && bun run build && bun run test:unit",
    "ci:full": "bun install && bun run build && bun test",
    "typecheck": "bunx tsc --noEmit",
    "typecheck:strict": "bunx tsc --noEmit --strict"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/react": "^3.0.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/dagre": "^0.7.53",
    "@xyflow/react": "^12.10.0",
    "ai": "^6.0.3",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dagre": "^0.8.5",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "playwright": "^1.57.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0"
  }
}
